            "Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/collections": "^10.29",
        "rakit/validation": "^1.4",
        "illuminate/pagination": "^10.29"
    },
    "require-dev": {
        "pestphp/pest": "^2.24",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
